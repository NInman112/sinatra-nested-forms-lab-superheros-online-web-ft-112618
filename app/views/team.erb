<h1>Super Teams</h1>
<div class="team">
  <p>Team Name: <%= @team.name %></p>
	<p>Team Motto: <%= @team.motto %></p>
  </div><br>

<h1>Super Heros</h1>
<% @members.each do |hero| %>
  <div class="hero">
    <p>Name: <%= hero.name %></p>
    <p>Hero Power: <%= hero.power %></p>
    <p>Bio: <%= hero.bio %></p><br>
  </div><br>
<% end %>
